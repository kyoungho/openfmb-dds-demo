MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: Message_subscriber Message_publisher

REMAINING_TARGETS := $(subst all, , $(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	@$(MAKE) -f GNUmakefile.Message_subscriber $(@)
	@$(MAKE) -f GNUmakefile.Message_publisher $(@)

.PHONY: Message_subscriber
Message_subscriber:
	@$(MAKE) -f GNUmakefile.Message_subscriber all

.PHONY: Message_publisher
Message_publisher: Message_subscriber
	@$(MAKE) -f GNUmakefile.Message_publisher all

project_name_list:
	@echo Message_publisher
	@echo Message_subscriber
