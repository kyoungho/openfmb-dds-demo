    stream, ${member.sampleRef}) ;
#*----*##end
#*--*##else
#*----*##if($member.strSize)
if (!${member.elementSerializeMethod}(
        stream, 
        (${member.sample}).c_str(), 
        #getStrSize($member "serialize") + 1)) {
